<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Button1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:backgroundTint">@color/verde1</item>
    </style>
    <style name="Button1_land">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:backgroundTint">@color/verde1</item>
    </style>

    <style name="Layout1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:alpha">0.6</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="buttonlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="strokeColor">@android:color/white</item>
    </style>
    <style name="buttonlined_land" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="strokeColor">@android:color/white</item>
    </style>
</resources>